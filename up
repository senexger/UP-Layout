partial alphanumeric_keys // modifier_keys keypad_keys TODO: muss das ausgeklammert?
xkb_symbols "unix-pool" {

    // Levels in Unix-Pool
    // --------------------------------------------------------------
    // Ebene 1: normal
    // Ebene 2: Shift
    // Ebene 3: Mod3
    // Ebene 4: Mod4 //TODO: (for marking something use Shift + Mod4)
    // Ebene 5: Shift + Mod3 

    // New Legend 
    // =============== 
    // Levels in Xkbmap jargon to be found here in the definitions.  
    // These are the levels used, and Xorg's translations:
    // --------------------------------------------------------------
    // Xorg:       Level1                   Level2                   Level3                   Level4                   Level5                   
    // up:         latters                  big latters              navigation               shift navigation         symbols		        
    // Keys (up):  None                     Shift                    Mod3                     Mod3 + Shift             Mod4                     

    // Alphanumeric-keys
    // ===============
    // Tab as Multi_key (Compose)
    // --------------------------------------------------------------
    key.type[Group1] = "EIGHT_LEVEL_ALPHABETIC_LEVEL_FIVE_LOCK";
    key  <TAB> { [ Tab,  	  	    Tab,                     Tab,         	      NoSymbol,                NoSymbol                 ] };
    key <TLDE> { [ grave,                   asciitilde,              U21BB,                   U02DE,                   dead_abovedot            ] };
    key <AE01> { [ 1,                       exclam,    	             onesuperior,             onesubscript,            ordfeminine              ] };
    key <AE02> { [ 2,                       at,   		     twosuperior,             twosubscript,            masculine                ] };
    key <AE03> { [ 3,                       numbersign, 	     threesuperior,           threesubscript,          numerosign               ] };
    key <AE04> { [ 4,                       dollar,      	     Home,                    Begin,                   NoSymbol                 ] };
    key <AE05> { [ 5,                       percent,      	     U2039,                   malesymbol,              periodcentered           ] };
    key <AE06> { [ 6,                       dead_circumflex,         cent,                    U26A5,                   sterling                 ] };
    key <AE07> { [ 7,                       ampersand,               yen,                     U03F0,                   currency                 ] };
    key <AE08> { [ 8,                       asterisk, 	             singlelowquotemark,      U27E8,                   Tab                      ] };
    key <AE09> { [ 9,                       parenleft, 	             leftsinglequotemark,     U27E9,                   KP_Divide                ] };
    key <AE10> { [ 0,                       parenright, 	     End,                     zerosubscript,           KP_Multiply              ] };
    key <AE11> { [ minus,                   underscore,              NoSymbol,                U2011,                   KP_Subtract              ] };
    key <AE12> { [ dead_grave,              plus,     	             dead_abovering,          dead_dasia,              dead_diaeresis           ] };

    // Top row
    // --------------------------------------------------------------
    key <AD01> { [ q,                       Q,                       ellipsis,                Greek_xi,                adiaeresis               ] };
							 	     //TODO: next word
    key <AD02> { [ w,                       W,                       XK_Prior,                NoSymbol,                BackSpace                ] };
    key <AD03> { [ e,                       E,                       bracketleft,             Greek_lambda,            Up                       ] };
    key <AD04> { [ r,                       R,                       bracketright,            Greek_chi,               Delete                   ] };
    key <AD05> { [ t,                       T,                       asciicircum,             Greek_omega,             Next                     ] };
    key <AD06> { [ y,                       Y,                       exclam,                  Greek_kappa,             udiaeresis               ] };
    key <AD07> { [ u,                       U,                       less,                    Greek_psi,               KP_7                     ] };
    key <AD08> { [ i,                       I,                       greater,                 Greek_gamma,             KP_8                     ] };
    key <AD09> { [ o,                       O,                       Linefeed,                Greek_phi,               KP_9                     ] };
    key <AD10> { [ p,                       P,                       ampersand,               U03D5,                   odiaeresis               ] };
    key <AD11> { [ ssharp,                  U1E9E,                   U017F,                   Greek_finalsmallsigma,   U2212                    ] };
    key <AD12> { [ dead_acute,              dead_tilde,              dead_stroke,             dead_psili,              dead_doubleacute         ] };

    // Middle row
    // --------------------------------------------------------------
    key <AC01> { [ a,                       A,                       backslash,               NoSymbol,                Home                     ] };
    key <AC02> { [ s,                       S,                       slash,                   ssharp,                  ssharp                   ] };
    key <AC03> { [ d,                       D,                       braceleft,               Greek_alpha,             Down                     ] };
    key <AC04> { [ f,                       F,                       braceright,              Greek_epsilon,           Right                    ] };
    key <AC05> { [ g,                       G,                       asterisk,                Greek_omicron,           End                      ] };
    key <AC06> { [ h,                       H,                       Left,                    leftarrow,               questiondown             ] };
    key <AC07> { [ j,                       J,                       Down,                    downarrow,               downarrow                ] };
    key <AC08> { [ k,                       K,                       Up,                      uparrow,                 KP_5                     ] };
    key <AC09> { [ l,                       L,                       Right,                   rightarrow,              KP_6                     ] };
    key <AC10> { [ semicolon,               colon,                   colon,                   Greek_delta,             KP_Separator             ] };
    key <AC11> { [ apostrophe,              quotedbl,                at,                      Greek_upsilon,           period                   ] };

    // Bottom row
    // --------------------------------------------------------------
    key <AB01> { [ z,    	            Z,         		     numbersign,              NoSymbol,                Escape                   ] };
    key <AB02> { [ x,    	            X,         		     dollar,                  U03F5,                   Tab                      ] };
    key <AB03> { [ c,    	            C,         		     bar,                     Greek_eta,               Insert                   ] };
    key <AB04> { [ v,    	            V,         		     asciitilde,              Greek_pi,                Return                   ] };
							 	     //TODO: previous word for lvl3 w
    key <AB05> { [ b,    	            B,         		     grave,                   Greek_zeta,              Undo                    ] };
    key <AB06> { [ n,    	            N,         		     plus,                    Greek_beta,              colon                   ] };
    key <AB07> { [ m,    	            M,         		     percent,                 Greek_mu,                KP_1                    ] };
    key <AB08> { [ comma,                   less,       	     quotedbl,                U03F1,                   KP_2                    ] };
    key <AB09> { [ period,                  greater,       	     apostrophe,              U03D1,                   KP_3                    ] };
    key <AB10> { [ slash,                   question,       	     semicolon,               Greek_theta,             semicolon               ] };

    // Space key
    // --------------------------------------------------------------
    key <SPCE> { [ space,                   space,                   space,                   nobreakspace,            KP_0                    ] };

    // TODO: I do not have a keypad-keys
    // TODO: what does this line?: key.type[Group1] = "EIGHT_LEVEL_ALPHABETIC_LEVEL_FIVE_LOCK";

    //key <RALT> { [ Multi_key, ISO_Level3_Shift, Multi_key ] };
    //key <CAPS> { type[Group1]="TWO_LEVEL", [ Multi_key ] };

    //modifier_map Mod3   { <CAPS> };
    //modifier_map Mod5   { <RALT> };

    // include "shift(both_capslock)" // can be removed?
    // include "level3(caps_switch)"
    // include "level3(bksl_switch)" // it's the neo inspired way`
};

// TODO: integrate this from variant to basic only

partial alphanumeric_keys modifier_keys keypad_keys
xkb_symbols "neo" {

    include "up(unix-pool)"

    name[Group1]= "Unix-Pool (US)";

    // include "shift(both_capslock)" // can be removed?
    include "level3(caps_switch)"
    include "level3(bksl_switch)" // it's the neo inspired way`
    // TODO: remove layer 6,7,8
    //include "level5(lsgt_switch_lock)"
    //include "level5(ralt_switch_lock)"
};

